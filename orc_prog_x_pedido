SELECT DISTINCT
    C.ID,
    CLI.RAZAOSOCIAL,
    TIPO.TIPO AS GRUPO,
    I.DATASOLIC,
    C.VALOR,
    C.ID_EMP
FROM
    COTACAOI I
    LEFT JOIN COTACAO C ON C.ID = I.ID_COTACAO
    LEFT JOIN CADCLIENTE CLI ON CLI.ID = C.ID_CLIENTE
    LEFT JOIN CADTIPOCLIENTE TIPO ON TIPO.ID = CLI.ID_TIPO
    LEFT JOIN CADCLIENTE_ENDERECO EN ON (EN.ID_CLIENTE = CLI.ID AND UPPER(EN.TIPO_END) = '(PADR√ÉO)')
WHERE
    I.STATUS = 'PROG'
    AND C.ID_RETIF IS NULL
